<h1>Your bookings:</h1>
<ul>
  <% @bookings.each do | booking | %>
    <li><%= booking.instrument.title %></li>
    <li><%= booking.start_date %></li>
    <li><%= booking.end_date %></li>
    <%= link_to "Delete", booking_path(booking), data: {
      turbo_method: :delete,
        turbo_confirm: "Are you sure?" } %>
  <% end %>
</ul>
